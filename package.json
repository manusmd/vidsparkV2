{
  "name": "vidspark",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_APP_ENV='emulator' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulators": "firebase emulators:start",
    "deploy:site": "npx remotion lambda sites create src/components/remotion/index.ts --site-name=vidspark-video --region=eu-central-1",
    "deploy:services": "npx remotion lambda functions deploy --region=eu-central-1 --memoryLimit=8Gi --cpuLimit=4.0  --timeoutSeconds=1000 --concurrency=10",
    "undeploy:site": "npx remotion cloudrun sites rm vidspark-video --region=us-central1",
    "undeploy:services": "npx remotion cloudrun services rm remotion-4-0-277-mem8gi-cpu4-t1000 --region=us-central1"
  },
